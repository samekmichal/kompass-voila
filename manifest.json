{
    "name": "Kompass.de Voila",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Description ...",
    "author": "Michal Samek, samek.michal <at> gmail.com",
    "icons": {
        "48": "icons/icon.png"
    },
    "permissions": ["tabs"],
    "content_scripts": [{
            "matches": ["<all_urls>"],
            "js": ["content.js", "jquery.js"]
        }],
    "page_action": {
        "default_icon": "icons/icon.png",
        "default_title": "Voila!"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}